const { isSameEpisode } = require('../utils');

/**
 * containsDupes - Determines if playlist has any duplicate Episodes
 *
 * @param  {type} playlist
 * @return {Boolean}
 */
function containsDupes(playlist = []) {
  for (let i = 0; i < playlist.length - 1; i += 1) {
    for (let j = i + 1; j < playlist.length; j += 1) {
      if (isSameEpisode(playlist[i], playlist[j])) return true;
    }
  }
  return false;
}

module.exports = containsDupes;
